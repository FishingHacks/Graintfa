{
	"name": "graintfa",
	"productName": "graintfa",
	"version": "1.0.0",
	"description": "Graintfa - A Graphical User Interface Design Tool for Minecraft.",
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint --ext .ts,.tsx ."
	},
	"keywords": [],
	"author": {
		"name": "RedCrafter07",
		"email": "gh@r07.dev"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "graintfa",
						"iconUrl": "D:\\coding\\McGui\\src\\assets\\Graintfa.ico"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"devContentSecurityPolicy": "connect-src 'self' http://localhost:736 'unsafe-eval'; img-src 'self' 'unsafe-eval' file: data:;",
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/index.html",
									"js": "./src/renderer.tsx",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.63",
		"@electron-forge/maker-deb": "^6.0.0-beta.63",
		"@electron-forge/maker-rpm": "^6.0.0-beta.63",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.63",
		"@electron-forge/maker-zip": "^6.0.0-beta.63",
		"@electron-forge/plugin-webpack": "6.0.0-beta.63",
		"@types/react": "^17.0.40",
		"@types/react-dom": "^17.0.13",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@vercel/webpack-asset-relocator-loader": "1.7.0",
		"css-loader": "^6.0.0",
		"electron": "17.1.2",
		"eslint": "^8.0.1",
		"eslint-plugin-import": "^2.25.0",
		"fork-ts-checker-webpack-plugin": "^6.0.1",
		"node-loader": "^2.0.0",
		"style-loader": "^3.0.0",
		"ts-loader": "^9.2.2",
		"typescript": "~4.5.4"
	},
	"dependencies": {
		"@mantine/core": "^4.2.7",
		"@mantine/dropzone": "^4.2.7",
		"@mantine/hooks": "^4.2.7",
		"@mantine/modals": "^4.2.7",
		"@mantine/notifications": "^4.2.7",
		"@mantine/prism": "^4.2.7",
		"@mantine/spotlight": "^4.2.7",
		"@tabler/icons": "^1.68.0",
		"@types/cors": "^2.8.12",
		"@types/discord-rpc": "^4.0.3",
		"@types/express": "^4.17.13",
		"autoprefixer": "^10.4.7",
		"body-parser": "^1.20.0",
		"chalk": "4.1.2",
		"copy-webpack-plugin": "^11.0.0",
		"cors": "^2.8.5",
		"discord-rpc": "^4.0.1",
		"electron-squirrel-startup": "^1.0.0",
		"express": "^4.18.1",
		"framer-motion": "^6.3.6",
		"node-json-db": "^1.5.0",
		"postcss": "^8.4.13",
		"postcss-loader": "^6.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"tailwindcss": "^3.0.24"
	}
}
